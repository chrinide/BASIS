  S_EXPONENTS           8.53241549 \
                        0.88433298 \
                        0.27036900
  P_EXPONENTS           6.06504269 \
                        1.38033553 \
                        0.31010979
  &END PP_BASIS
  &POTENTIAL
    PSEUDO_TYPE GTH
    &GTH_POTENTIAL
    2    4    0    0
    0.24671011902360       2  -16.65533253748591    2.50854752353111
       1
    0.22100154713718       1   18.34370453997725
    &END 
    CONFINEMENT_TYPE  BARRIER
    CONFINEMENT 200. 4.0 12.00
  &END POTENTIAL

  &POWELL
     ACCURACY   1.e-14
     STEP_SIZE  0.10
     MAX_FUN    1000
  &END
&END ATOM
