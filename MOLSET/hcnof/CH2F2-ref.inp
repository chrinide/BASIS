&FORCE_EVAL
  METHOD Quickstep
  &DFT
    BASIS_SET_FILE_NAME BASIS_MOLOPT 
    POTENTIAL_FILE_NAME POTENTIAL
    &MGRID
      CUTOFF 600
      REL_CUTOFF  60
    &END MGRID
    &QS
      EPS_DEFAULT 1.0E-12
      MAP_CONSISTENT
      EXTRAPOLATION PS
      EXTRAPOLATION_ORDER 3
    &END QS
    &SCF
      EPS_SCF 1.0E-7
      SCF_GUESS ATOMIC
      MAX_SCF 100
      &OT
         MINIMIZER DIIS
         PRECONDITIONER FULL_ALL
      &END OT
    &END SCF
    &XC
      &XC_FUNCTIONAL PBE
      &END XC_FUNCTIONAL
    &END XC
  &END DFT
  &SUBSYS
    &CELL
      ABC 12.0 12.0 12.0
    &END CELL
    &COORD
  C        -0.000000       -0.000000        0.064384
  H         0.651066        0.651066        0.665388
  H        -0.651066       -0.651066        0.665388
  F        -0.795026        0.795026       -0.732818
  F         0.795026       -0.795026       -0.732818
    &END COORD
    &KIND H
      BASIS_SET TZV2PX-MOLOPT-GTH
      POTENTIAL GTH-PBE-q1
    &END KIND
    &KIND C
      BASIS_SET TZV2PX-MOLOPT-GTH
      POTENTIAL GTH-PBE-q4
    &END KIND
    &KIND f
      BASIS_SET TZV2PX-MOLOPT-GTH
      POTENTIAL GTH-PBE-q7
    &END KIND
  &END SUBSYS
&END FORCE_EVAL
&GLOBAL
  PROJECT refCH2F2
  RUN_TYPE ENERGY
  PRINT_LEVEL LOW
&END GLOBAL
