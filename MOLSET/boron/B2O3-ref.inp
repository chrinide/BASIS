&FORCE_EVAL
  METHOD Quickstep
  &DFT
    BASIS_SET_FILE_NAME BASIS_MOLOPT 
    POTENTIAL_FILE_NAME POTENTIAL
    &MGRID
      CUTOFF 500
      REL_CUTOFF  60
    &END MGRID
    &QS
      EPS_DEFAULT 1.0E-12
      MAP_CONSISTENT
      EXTRAPOLATION PS
      EXTRAPOLATION_ORDER 3
    &END QS
    &SCF
      EPS_SCF 1.0E-7
      SCF_GUESS ATOMIC
      MAX_SCF 100
      &OT
         MINIMIZER DIIS
         PRECONDITIONER FULL_ALL
      &END OT
    &END SCF
    &XC
      &XC_FUNCTIONAL PBE
      &END XC_FUNCTIONAL
    &END XC
  &END DFT
  &SUBSYS
    &CELL
      ABC 12.0 12.0 12.0
    &END CELL
    &COORD
  B         0.0000        0.0150       -0.7836
  B         0.0000        0.0150        0.7836
  O         0.0000        1.3272        0.0000
  O         0.0000        0.4488       -2.0041
  O         0.0000        0.4488        2.0041
    &END COORD
    &KIND B
      BASIS_SET DZVP-MOLOPT-SR-GTH
      POTENTIAL GTH-PBE-q3
    &END KIND
    &KIND O
      BASIS_SET DZVP-MOLOPT-SR-GTH
      POTENTIAL GTH-PBE-q6
    &END KIND
  &END SUBSYS
&END FORCE_EVAL
&GLOBAL
  PROJECT refB2O3
  RUN_TYPE ENERGY
  PRINT_LEVEL LOW
&END GLOBAL
